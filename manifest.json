{
    "manifest_version": 3,
    "name": "first_extention",
    "description": "My first extention",
    "version": "0.1.0",
    "action": {
        "default_icon": {
            "16": "images/icon16.png",
            "24": "images/icon32.png",
            "32": "images/icon48.png",
            "128": "images/icon128.png"
        },
        "default_title": "My YT Bookmarks",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "https://*.youtube.com/*"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*.youtube.com/*"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "images/add.png",
                "images/play.png",
                "images/delete.png",
                "images/save.png"
            ],
            "matches": [
                "https://*.youtube.com/*"
            ]
        }
    ]
}